public class ImpostoVeiculosMotorizados extends Imposto {
    private String marca;
    private String modelo;
    private String placa;

   
    public ImpostoVeiculosMotorizados(int id, Cliente cliente, double valor, String dataPagamento, String marca, String modelo, String placa, String nome, String cpf, Endereco endereco, String telefone, String email) {
        super(id, cliente, valor, dataPagamento);
        this.marca = marca;
        this.modelo = modelo;
        this.placa = placa;
    }


    public String getMarca() {
        return marca;
    }

    public void setMarca(String marca) {
        this.marca = marca;
    }

    public String getModelo() {
        return modelo;
    }

    public void setModelo(String modelo) {
       this.modelo = modelo;
    }

    public String getPlaca() {
        return placa;
    }

    public void setPlaca(String placa) {
        this.placa = placa;
    }
}